    <PropertyRef Id="VS2012_ROOT_FOLDER"/>
    <PropertyRef Id="VS2013_ROOT_FOLDER"/>
    <PropertyRef Id="VS2015_ROOT_FOLDER"/>
    <Condition Message="Visual Studio version 2012, 2013, 2015 or 2017 must be installed.">
