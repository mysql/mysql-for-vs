<UserControl x:Class="MySql.Data.VisualStudio.Editors.TableEditorWPF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MySql.Data.VisualStudio.Editors"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="MainGrid" Loaded="Grid_Loaded">
        <TabControl x:Name="ColumnPropertiesTabControl" HorizontalAlignment="Left" Margin="10,172,10,10" VerticalAlignment="Top">
            <TabItem x:Name="ColumnPropertiesTab" Header="Column Properties" Loaded="ColumnPropertiesTab_Loaded">
                <Grid Name="TabGrid" Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <WrapPanel x:Name="ColumnPanel" HorizontalAlignment="Left" Height="157" Margin="10,10,0,0" VerticalAlignment="Top" Width="374"/>
    </Grid>
</UserControl>
