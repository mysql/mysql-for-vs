#include "stdidcmd.h"
#include "vsshlids.h"
#include "msobtnid.h"
#include "Guids.h"
#include "PkgCmdID.h"

#define DIS_DEF DEFAULTDISABLED | DEFAULTINVISIBLE | DYNAMICVISIBILITY | DONTCACHE
#define OI_NOID guidOfficeIcon:msotcidNoIcon

CMDS_SECTION guidMySqlProviderPkg

	MENUS_BEGIN
		guidMySqlProviderCmdSet:menuAddNew2005, guidVS2005Data:8706, 0x1000,, "MySQLAdd", "&Add New";
		guidMySqlProviderCmdSet:menuAddNew2008, guidVSData:IDG_DV_GLOBAL1, 0x1000,, "MySQLAdd", "&Add New";
	MENUS_END

	NEWGROUPS_BEGIN
		guidMySqlProviderCmdSet:groupAddNew2005, guidMySqlProviderCmdSet:menuAddNew2005, 0x0000;
		guidMySqlProviderCmdSet:groupAddNew2008, guidMySqlProviderCmdSet:menuAddNew2008, 0x0000;
	NEWGROUPS_END

    BUTTONS_BEGIN
        guidMySqlProviderCmdSet:cmdidCreateTable, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "Create Table";

        guidMySqlProviderCmdSet:cmdidCreateView, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "Create View";
        guidMySqlProviderCmdSet:cmdidAlterView,  Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Alter View";

        guidMySqlProviderCmdSet:cmdidCreateProcedure, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Create Routine";
        guidMySqlProviderCmdSet:cmdidAlterProcedure, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Alter Routine";

        guidMySqlProviderCmdSet:cmdidCreateFunction, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Create Routine";

        guidMySqlProviderCmdSet:cmdidCreateTrigger, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Create Trigger";
        guidMySqlProviderCmdSet:cmdidAlterTrigger, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Alter Trigger";

        guidMySqlProviderCmdSet:cmdidCreateUDF, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Create UDF";

		guidMySqlProviderCmdSet:cmdidDelete, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Delete";

		/* global data menu items */
        guidMySqlProviderCmdSet:cmdidAddNewTableGlobal, Group_Undefined:0, 0x0000, OI_NOID, BUTTON, DIS_DEF, "&Table";
        guidMySqlProviderCmdSet:cmdidAddNewViewGlobal, Group_Undefined:0, 0x0001, OI_NOID, BUTTON, DIS_DEF, "Vie&w";
        guidMySqlProviderCmdSet:cmdidAddNewProcedureGlobal, Group_Undefined:0, 0x0002, OI_NOID, BUTTON, DIS_DEF, "Stored &Procedure";
        guidMySqlProviderCmdSet:cmdidAddNewFunctionGlobal, Group_Undefined:0, 0x0003, OI_NOID, BUTTON, DIS_DEF, "Stored &Function";
        guidMySqlProviderCmdSet:cmdidAddNewUDFGlobal, Group_Undefined:0, 0x0004, OI_NOID, BUTTON, DIS_DEF, "&UDF";
    BUTTONS_END

CMDS_END

CMDPLACEMENT_SECTION

	guidMySqlProviderCmdSet:cmdidDelete,  guidVSData:IDG_DV_OBJECT, 0x0999;
	
    guidMySqlProviderCmdSet:cmdidCreateTable,  guidVSData:IDG_DV_STATIC, 0x0100;

    guidMySqlProviderCmdSet:cmdidCreateView,  guidVSData:IDG_DV_STATIC, 0x0100;
    guidMySqlProviderCmdSet:cmdidAlterView,  guidVSData:IDG_DV_OBJECT, 0x0101;

    guidMySqlProviderCmdSet:cmdidCreateProcedure, guidVSData:IDG_DV_STATIC, 0x0100;
    guidMySqlProviderCmdSet:cmdidCreateProcedure, guidVSData:IDG_DV_CONNECTION, 0x0100;
    guidMySqlProviderCmdSet:cmdidAlterProcedure,  guidVSData:IDG_DV_OBJECT, 0x0100;

    guidMySqlProviderCmdSet:cmdidCreateFunction, guidVSData:IDG_DV_STATIC, 0x0100;

    guidMySqlProviderCmdSet:cmdidCreateTrigger, guidVSData:IDG_DV_OBJECT, 0x0100;
    guidMySqlProviderCmdSet:cmdidAlterTrigger, guidVSData:IDG_DV_OBJECT, 0x0100;

	guidMySqlProviderCmdSet:cmdidCreateUDF, guidVSData:IDG_DV_STATIC, 0x0100;

	// global data menu junk
	guidDataCmdId:13608, guidVSData:IDG_DV_GLOBAL1, 0x1001;
	guidDataCmdId:13608, guidVS2005Data:8706, 0x1001;
	
    guidMySqlProviderCmdSet:cmdidAddNewTableGlobal, guidMySqlProviderCmdSet:groupAddNew2005, 1;
    guidMySqlProviderCmdSet:cmdidAddNewViewGlobal, guidMySqlProviderCmdSet:groupAddNew2005, 2;
    guidMySqlProviderCmdSet:cmdidAddNewProcedureGlobal, guidMySqlProviderCmdSet:groupAddNew2005, 3;
    guidMySqlProviderCmdSet:cmdidAddNewFunctionGlobal, guidMySqlProviderCmdSet:groupAddNew2005, 4;
    guidMySqlProviderCmdSet:cmdidAddNewUDFGlobal, guidMySqlProviderCmdSet:groupAddNew2005, 5;

    guidMySqlProviderCmdSet:cmdidAddNewTableGlobal, guidMySqlProviderCmdSet:groupAddNew2008, 1;
    guidMySqlProviderCmdSet:cmdidAddNewViewGlobal, guidMySqlProviderCmdSet:groupAddNew2008, 2;
    guidMySqlProviderCmdSet:cmdidAddNewProcedureGlobal, guidMySqlProviderCmdSet:groupAddNew2008, 3;
    guidMySqlProviderCmdSet:cmdidAddNewFunctionGlobal, guidMySqlProviderCmdSet:groupAddNew2008, 4;
    guidMySqlProviderCmdSet:cmdidAddNewUDFGlobal, guidMySqlProviderCmdSet:groupAddNew2008, 5;
	
CMDPLACEMENT_END


